{
  "manifest_version": 2,
  "name": "DNS of HTTPS",
  "version": "0.1",

  "author": "www.linkedin.com/in/kevin-bolton",
  "description": "Let browser using private IP, and then DoH public IP.",
  "short_name": "DoH",
  "icons": {
    "48": "icons/ExtensionIcon.png"
  },
  
  "permissions": [
    "dns",
    "tabs"
  ],

  "browser_action": {
    "default_icon": {
      "16": "icons/button-16.png",
      "32": "icons/button-32.png"
    },
    "default_title": "DNS of HTTPS"
  },

  "background": {
    "scripts": ["background_script/DoH.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/check.js"],
      "run_at": "document_end"
    }
  ]
}